#include<stdio.h>
int main (void)
{
    int t,x,y,i,res;
    scanf("%d",&t);
    for(i = 0; i < t; i++){
        scanf("%d%d",&x,&y);
        res = abs(x - y);
        if (x >= y ) printf("%d\n",res);
        else if (x < y) printf("0\n");
        else  if ("x = y")printf("0\n");

    }
    return 0;
}
