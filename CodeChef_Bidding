#include <stdio.h>

int main(void) 
{
    int a,b,c,t,i;
    scanf("%d",&t);
    
    for (i = 0; i < t; i++)
    {
        scanf("%d %d %d",&a,&b,&c);
        if (a > b && a > c) printf("Alice\n");
        
        else if (a < b && c < b) printf("Bob\n");
        
        else if (a < c && c > b) printf("Charlie\n");
    }
    
    return 0;
}

