#include <stdio.h>

int main()
{
	int t, n, x, p;

	scanf("%d",&t);

	for (int i = 0; i<t;i++){

	    scanf("%d%d%d",&n,&x,&p);

	    int inc = n - x;
	    int tot = x * 3;
	    int inc_tot =  tot - inc;


	    if(inc_tot >= p){
	        printf("PASS\n");

	    }
        else if(inc < p){
            printf("FAIL\n");
        }
         else if(inc_tot < p){
	        printf("FAIL\n");

         }
	}
	return 0;

}



